<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Template"
    jcr:title="Havells SubCategory (Ranges) page Template "
    jcr:description="Havells generic subcategory (Ranges) page template."
    allowedPaths="[/content/havells/[^/]+(/.*)?,/content/catalogs/havells/templatePages(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="havells/components/page/subCategoryPage">
        <rightPar jcr:primaryType="nt:unstructured"
                  sling:resourceType="foundation/components/parsys">
            <subCategoryList
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="havells/components/commerce/categoryListNew">
            </subCategoryList>
        </rightPar>
    </jcr:content>
</jcr:root>
