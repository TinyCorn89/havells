<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="PrintCoverageWrapper"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="wrapper1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enter Title "
                        name="./title"
                        xtype="textfield"/>
                    <errormsg
                            jcr:primaryType="cq:Widget"
                            fieldLabel="Enter Error Message "
                            name="./errmsg"
                            xtype="textfield"/>
                    <pagination
                        jcr:primaryType="cq:Widget"
                        fieldLabel="No. of Publication / Page"
                        name="./pagination"
                        xtype="textfield"/>
                    <filter
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Year Filter"
                        name="./filter"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            name="./filteryear"
                            width="155"
                            xtype="textfield"/>
                    </filter>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
